    "license": "Discovery Commercial",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "buildStaging": "ng build --configuration=staging",
        "test": "ng test",
        "lint": "ng lint",
        "buildProd": "ng build --prod --base-href=/ga/",
        "buildStandalone": "ng build",
        "serveStandaloneWatch": "ng serve --proxy-config proxy-dev.json --watch=true",
        "buildDevToSpring": "ng build --base-href=/ga/static/ --output-path=./backend/src/main/resources/static",
        "buildProdToSpring": "ng build --prod --base-href=/ga/static/ --output-path=./backend/src/main/resources/static",
        "build-accelengine-lib": "ng build accelengine-lib --prod && cd ./dist/accelengine-lib/ && npm pack",
        "link-lib": "cd ./dist/accelengine-lib/ && npm link",
        "build-watch-lib": "ng build accelengine-lib --watch",
        "check-updates": "ncu -u",
        "publish-accelengine-lib": "ng build accelengine-lib --prod && cd ./dist/accelengine-lib/ && npm publish --registry http://10.10.20.31:8081/repository/npm-private",
        "sonar": "sonar-scanner"
    },
    "private": false,
